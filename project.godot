; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Fried Dungeons"
run/main_scene="res://assets/game_manager/game_manager.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
run/max_fps=240
config/icon="res://icon.svg"

[autoload]

Utils="*res://autoloads/utils/utils.gd"
Game="*res://autoloads/game/game.gd"
Debug="*res://autoloads/debug/debug.gd"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[debug]

multirun/number_of_windows=2
multirun/add_custom_args=true
multirun/window_args=["listen", "join"]

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/mode=2
window/size/initial_position_type=2
window/size/initial_screen=1

[editor_plugins]

enabled=PackedStringArray("res://addons/LineRenderer/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/dice_syntax/plugin.cfg", "res://addons/multirun/plugin.cfg", "res://addons/sprite_mesh/plugin.cfg", "res://addons/state_machine_nodes/plugin.cfg", "res://addons/tnowe_extra_controls/plugin.cfg")

[global_group]

info=""
base=""
entities=""

[gui]

theme/custom="res://resources/themes/main/main_theme.tres"
theme/default_font_subpixel_positioning=0
theme/lcd_subpixel_layout=0

